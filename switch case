#include <stdio.h>

int main()
{ 
    int a,b,c,d,e,f,g,ch;
    printf("enter the two  variables a and b here -->");
    scanf("%d %d",&a,&b);
    printf("enter the operation \n 1.addition \n 2.difference \n 3.multiplication \n 4.divison \n 5.modules");
    scanf("%d" ,&ch);
    switch(ch)
    {
        case 1:
        c=a+b;
        printf("sum of two numbers is %d ",c);
        break;
        case 2:
        d=a-b;
        printf("difference of two numbers is %d ",d);
        break;
        case 3:
        e=a*b;
        printf("multiplication of two numbers is %d ",e);
        break;
        case 4:
        f=a/b;
        printf("divison of two numbers is %d ",f);
        break;
        case 5:
        g=a%b;
        printf("modulesof two numbers is %d ",g);
        break;
        default:
        printf("error!! enter correct choice ");
        
    }


}


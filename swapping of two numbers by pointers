#include <stdio.h>

int main()
{
    int a,b,*c,*d,temp;
    
    printf("enter the value of a: ");
    scanf("%d",&a);
    printf("enter the value of b: ");
    scanf("%d",&b);
    
    printf("before swapping \na=%d \nb=%d \n",a,b);
    
    c=&a;
    d=&b;
    temp=b;
    b=a;
    a=temp;
    
    printf("\nafter swapping\na=%d \nb=%d",a,b);


    return 0;
}

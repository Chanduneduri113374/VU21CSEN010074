#include <stdio.h>

int main()
{int side1, side2, side3;
   printf("enter the side 1 of triangle : ");
   scanf("%d",&side1);
   printf("enter the side 2 of triangle : ");
   scanf("%d",&side2);
   printf("enter the side 3 of triangle : ");
   scanf("%d",&side3);
   if(side1 == side2 && side2 == side3)
      printf("The Given Triangle is equilateral\n");
   else if(side1 == side2 || side2 == side3 || side3 == side1)
      printf("The given Triangle is isosceles\n");
   else
      printf("The given Triangle is scalene\n");

    return 0;
}

#include <stdio.h>

int main()
{
    int i,j,n;
    printf("enter the any number which you want untill to prime number: ");
    scanf("%d",&n);
    
    printf("the prime numbers are: \n");
    for(i=2;i<=n;i++)
    {
       int c=0;
       for(j=1;j<=i;j++)
       {
        if(i%j==0)
        {
           c++;
           
        }
       }
       if(c==2){
          printf("%d \n",i);
       }
       
       
    }

    return 0;
}

